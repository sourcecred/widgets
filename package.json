  "private": true,
  "dependencies": {
    "esm": "^3.2.25",
    "got": "^10.2.2",
    "keyv-fs-sync": "^0.1.0",
    "mkdirp": "^1.0.3",
    "sharp": "^0.25.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "flow-bin": "^0.120.1",
    "jest": "^25.1.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0"
  },
  "scripts": {
    "build": "yarn -s build:clean && yarn -s build:lib",
    "build:clean": "rimraf lib",
